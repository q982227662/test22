      "@fortawesome/fontawesome-free": "~5.15",
      "@typescript-eslint/eslint-plugin": "~4.4",
      "@typescript-eslint/parser": "~4.4",
      "babel-minify": "~0.5",
      "canvas": "~2.6",
      "dna.js": "~1.6",
      "eslint": "~7.12",
      "gulp": "~4.0",
      "gulp-append-prepend": "~1.0",
      "gulp-babel": "~8.0",
      "gulp-header": "~2.0",
      "gulp-htmlhint": "~3.0",
      "gulp-rename": "~2.0",
      "gulp-replace": "~1.0",
      "gulp-size": "~3.0",
      "gulp-w3c-html-validator": "~2.0",
