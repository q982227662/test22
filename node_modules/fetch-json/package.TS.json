      "Byron Becker (https://github.com/ByronBecker)"
   ],
   "keywords": [
      "browser",
      "fetch",
      "get",
      "http",
      "jamstack",
      "javascript",
      "json",
      "node-fetch",
      "post",
      "rest"
   ],
   "jshintConfig": {
      "esversion": 9,
      "strict": "implied",
      "eqeqeq": true,
      "undef": true,
